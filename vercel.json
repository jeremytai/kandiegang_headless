{
  "buildCommand": "npm run build",
  "framework": null,
  "rewrites": [
    { "source": "/api/:path*", "destination": "/api/:path*" },
    { "source": "/((?!assets|fonts|images|logos|robots.txt|_redirects).*)", "destination": "/index.html" }
  ],
  "functions": {
    "pages/api/**/*.ts": {
      "runtime": "@vercel/node@3"
    }
  }
}
